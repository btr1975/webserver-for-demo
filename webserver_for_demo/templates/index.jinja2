{% extends "base.jinja2" %}

{% block content %}

<div>
<h2>Request Info</h2>
<table>
    <tr>
        <th>KEY</th>
        <th>VALUE</th>
    </tr>
{% for key, value in data.request_info.items() %}
    <tr>
        <td>{{ key }}</td>
        <td>{{ value }}</td>
    </tr>
{% endfor %}
 </table>
</div>

<div>
<h2>Request Headers</h2>
<table>
    <tr>
        <th>KEY</th>
        <th>VALUE</th>
    </tr>
{% for key, value in data.request_headers.items() %}
    <tr>
        <td>{{ key }}</td>
        <td>{{ value }}</td>
    </tr>
{% endfor %}
 </table>
</div>

{% endblock %}